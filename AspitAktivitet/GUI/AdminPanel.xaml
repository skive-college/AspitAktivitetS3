<UserControl x:Class="AspitAktivitet.GUI.AdminPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AspitAktivitet.GUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Button x:Name="cmdLogout" Content="Log ud" Grid.Row="0" Margin="0 10 10 -20" Panel.ZIndex="10" Click="CmdLogout_Click"/>
        <TabControl x:Name="TabControler" Grid.Row="1" Height="360" SelectionChanged="TabControler_SelectionChanged">
            <TabItem x:Name="TabPlan" Header="Plan" >                               
            </TabItem>
            <TabItem x:Name="TabBruger" Header="Bruger">
            </TabItem>
            <TabItem x:Name="TabAktivitet" Header="Aktiviteter">                
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
