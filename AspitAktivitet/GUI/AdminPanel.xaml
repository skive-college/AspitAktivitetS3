<UserControl x:Class="AspitAktivitet.GUI.AdminPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AspitAktivitet.GUI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TabControl Height="360">
            <TabItem Header="Plan">
                <local:AdminPlans/>
            </TabItem>
            <TabItem Header="Bruger">
                <local:AdminUsers/>
            </TabItem>
            <TabItem Header="Aktiviteter">
                <local:AdminActivities/>
            </TabItem>
        </TabControl>
        <Button x:Name="cmdLogout" Content="Log ud" VerticalAlignment="Bottom" Click="CmdLogout_Click"/>
    </StackPanel>
</UserControl>
